type InsertError = variant {
  ValueTooLarge : KeyTooLarge;
  KeyTooLarge : KeyTooLarge;
};
type InsertResult = variant { ok : opt text; err : InsertError };
type KeyTooLarge = record { max : nat32; given : nat32 };
service : () -> {
  contains_key : (text) -> (bool) query;
  get : (text) -> (opt text) query;
  insert : (text, text) -> (InsertResult);
  is_empty : () -> (bool) query;
  len : () -> (nat64) query;
  remove : (text) -> (opt text);
}