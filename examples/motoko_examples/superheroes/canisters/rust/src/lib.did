type SuperheroId = nat32;

type Superhero = record {
    name: text;
    superpowers: opt List;
};

type List = record {
    text;
    opt List;
};

service : {
    create: (Superhero) -> (SuperheroId);
    delete: (SuperheroId) -> (bool);
    read: (SuperheroId) -> (opt Superhero) query;
    update: (SuperheroId, Superhero) -> (bool);
}
