{
    "canisters": {
        "azle": {
            "type": "custom",
            "build": "npx azle azle",
            "root": "canisters/azle",
            "ts": "canisters/azle/index.ts",
            "candid": "canisters/azle/index.did",
            "wasm": "target/wasm32-unknown-unknown/release/azle.wasm",
            "declarations": {
                "output": "dfx_generated/azle"
            }
        },
        "www": {
            "dependencies": ["azle"],
            "frontend": {
                "entrypoint": "canisters/www/index.html"
            },
            "source": ["canisters/www/components", "dist/www/"],
            "type": "assets"
        }
    }
}
