{
    "canisters": {
        "minimal_dapp": {
            "type": "custom",
            "build": "npx azle minimal_dapp",
            "root": "src",
            "ts": "src/minimal_dapp/main.ts",
            "candid": "src/minimal_dapp/main.did",
            "wasm": "target/wasm32-unknown-unknown/release/minimal_dapp.wasm",
            "declarations": {
                "output": "test/dfx_generated/minimal_dapp"
            }
        },
        "minimal_dapp_assets": {
            "dependencies": ["minimal_dapp"],
            "frontend": {
                "entrypoint": "src/minimal_dapp_assets/src/index.html"
            },
            "source": [
                "src/minimal_dapp_assets/assets",
                "dist/minimal_dapp_assets/"
            ],
            "type": "assets"
        }
    }
}
