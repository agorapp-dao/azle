type rec_1 = record {id:text};
type rec_2 = record {id:text; balance:nat64};
type rec_3 = record {id:text; balance:nat64};
service: () -> {
    transfer: (text, text, nat64) -> (nat64);
    balance: (text) -> (nat64) query;
    account: (rec_1) -> (opt rec_2) query;
    accounts: () -> (opt rec_3) query;
    trap: () -> (text) query;
    receiveNotification: (text) -> ();
    getNotification: () -> (text) query;
}
