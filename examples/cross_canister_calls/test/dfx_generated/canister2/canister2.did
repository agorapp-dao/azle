type AccountArgs = record {
    "id": text;
};

type Account = record {
    "id": text;
    "balance": nat64;
};

service: {
    "balance": (text) -> (nat64) query;
    "account": (AccountArgs) -> (opt Account) query;
    "accounts": () -> (vec Account) query;
    "trap": () -> (text) query;
    "get_notification": () -> (text) query;
    "transfer": (text, text, nat64) -> (nat64);
    "receive_notification": (text) -> ();
}
